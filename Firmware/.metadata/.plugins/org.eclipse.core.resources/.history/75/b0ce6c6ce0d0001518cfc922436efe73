#include "stm32f0xx.h"
#include <stdio.h>
#include <stdlib.h>

void HardwareSetup(void)
{
	// enble the GPIOA clock
	RCC->AHBENR |= RCC_AHBENR_GPIOAEN;




}

int main(void)
{
	HardwareSetup();

  // Infinite loop
  for( ;; )
    {
       // Add your code here.
    }
}

